// 通过检查，查看元素样式声明
.page .page-title {
    display: none;
}

strong {
    padding: 0.25rem 0.5rem;
    margin: 0;
    font-size: 0.85em;
    font-weight: bold !important;
    border-radius: 3px;
    color: #ff2c00;
}

blockquote {
    color: #666 !important;
}

.dark .content__default strong {
    background-color: rgba(58,58,92,0.7);
    color: #fff;
}

.dark .content__default blockquote {
    color: #999 !important;
}